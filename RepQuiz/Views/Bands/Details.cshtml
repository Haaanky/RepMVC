@model Band

@{
    ViewBag.Title = "Details band page";
}
<div>
    <h2>@Model.BandName</h2>
    <p>@Model.Description</p>
    <img src="@Model.ImagePath" alt="Picture of @Model.BandName" />
    <iframe width="914" height="514" src="@Model.YouTubeLink" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    <ul>
        @foreach (var item in @Model.Albums)
        {
            <li>@item</li>
        }
    </ul>
</div>